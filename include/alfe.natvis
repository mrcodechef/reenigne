      <CustomListItems MaxItemsPerView="5000">
        <Variable Name="p" InitialValue="($T1*)(((char*)this) + 16)" />
        <Size>_size</Size>
        <Loop>
          <If Condition="p->_key._length != 0">
            <Item>p,na</Item>
          </If>
          <Exec>++p</Exec>
        </Loop>
